<header id="top-header">
  
  <!-- DEBUG: loggedIn = <%= loggedIn %> -->
   <% console.log('DEBUG: loggedIn =', loggedIn); %>
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (loggedIn) { %>
      <span class="welcome-msg">
        Welcome,<a title="Click to log out" href="/account/"> <%= accountData.account_firstname %>!</a>
      </span>
      <a title="Click to log out" href="/account/logout">Logout</a>
    <% } else { %>
      <a title="Click to log in" href="/account/login">My Account</a>
    <% } %>
  </div>

  
</header>
